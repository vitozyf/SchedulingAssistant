.dialog-wapper {
  width: 100vw;
  height: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: 9999;
  background-color: rgba(0, 0, 0, 0.3);
}
.dialog {
  background-color: #fff;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translateX(-50%) translateY(-50%);
  min-width: 500rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.dialog-title {
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 10rpx;
  /* border-bottom: 1px solid #e6e6e6; */
  background-color: #f8f8f8;
}
.dialog-body {
  padding: 10rpx;
}
.dialog-footer {
  display: flex;
  padding: 0 10rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1px solid #e6e6e6;
}
.footer-btn {
  flex: 1;
  box-sizing: border-box;
  text-align: center;
}
.confirm-btn {
  color: #1aad19;
}
.cancel-btn {
  border-right: 1px solid #e6e6e6;
  color: #666;
}
